{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cleanmymac",
  "title": "CleanMyMac",
  "description": "Quick shortcut commands for CleanMyMac",
  "icon": "command-icon.png",
  "author": "juwul",
  "categories": [
    "System",
    "Productivity",
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "empty-bin",
      "title": "Empty Bin",
      "subtitle": "CleanMyMac",
      "description": "Empties the MacOS bin with CleanMyMac without opening the application.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.3",
    "run-applescript": "^6.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
